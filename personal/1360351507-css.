/**** @group blog filter ****/

#blog #filter {
  padding: 10px 0;
  margin-bottom: 20px;
  overflow: hidden;
  width: 800px;
  border-bottom: 1px solid rgba(255, 255, 255, 0.25);
  min-height: 22px;
}

#blog #filter .blog-crumbs {
  float: left;
  width: 660px;
  padding-bottom: 0;
  font-size: 11px;
  color: #a6a6a6;
  line-height: 22px;
  display: none;
}
  #blog.category #filter .blog-crumbs, #blog.tag #filter .blog-crumbs, #blog.single #filter .blog-crumbs {
    display: block;
  }
    #blog #filter .blog-crumbs a {
      margin-right: 5px;
      text-decoration: none;
      color: #a6a6a6;
    }
      #blog #filter .blog-crumbs a:hover {
        color: #d46819;
      }
    #blog #filter .blog-crumbs span:before {
      content: "\2192";
      padding-right: 5px;
    }
    .ie6 #blog #filter .blog-crumbs a:after, .ie7 #blog #filter .blog-crumbs a:after {
      content: "";
    }
    #blog #filter .blog-crumbs a:hover:after {
      color: #a6a6a6;
    }

#blog #filter .category_dropdown {
  float: right;
  width: 140px;
  position: relative;
}
  #blog #filter .category_dropdown .select-box, #blog #filter .category_dropdown select {
    position: absolute;
    top: 0;
    left: 0;
    cursor: pointer;
  }
    #blog #filter .category_dropdown .select-box {
      background: #d46819;
      color: #ffffff;
      z-index: 0;
      font-size: 11px;
      line-height: 16px;
      width: 130px;
      padding: 3px 5px;
      -webkit-border-radius: 3px;
         -moz-border-radius: 3px;
              border-radius: 3px;
    }
      #blog #filter .category_dropdown .select-box span {
        float: left;
      }
      #blog #filter .category_dropdown .select-box .arrow {
        font-size: 8px;
        line-height: 2em;
        float: right;
      }
      .ie6 #blog #filter .category_dropdown .select-box {
        display: none;
      }
    #blog #filter .category_dropdown select {
      opacity: 0;
      filter:alpha(opacity=0);
      z-index: 10;
      width: 140px;
      border: none;
      padding: 1px 0;
    }
      #blog #filter .category_dropdown select option {
        font-size: 11px;
        padding: 2px 0;
      }

/**** @end blog filter ****/

/**** @group blog categories tags comments ****/

#blog .entry .comments-tags-categories {
  font-size: .9em;
  line-height: 17px;
  margin-top: 15px;
  overflow: hidden;
  color: rgb(255, 255, 255);
  color: rgba(255, 255, 255, 0.4);
}
  #blog .entry .comments-tags-categories .label {
    color: #d46819;
    margin-right: 3px;
    font-weight: bold;
  }
  #blog .entry .comments-tags-categories .comments, #blog .entry .comments-tags-categories .categories, #blog .entry .comments-tags-categories .tags {
    margin-right: 8px;
  }
  #blog .entry .comments-tags-categories .comments, #blog .entry .comments-tags-categories .categories, #blog .entry .comments-tags-categories .tags {
    float: left;
    margin-bottom: 5px;
    padding: 3px 5px;
    background: rgb(255, 255, 255);
    background: rgba(255, 255, 255, 0.1);
    -webkit-border-radius: 3px;
       -moz-border-radius: 3px;
            border-radius: 3px;
  }
  #blog .entry .comments-tags-categories a {
    color: rgb(255, 255, 255);
    color: rgba(255, 255, 255, 0.4);
    text-decoration: none;
  }
  #blog .entry .comments-tags-categories .comments a {
    color: #d46819;
  }
  #blog .entry .comments-tags-categories a:hover {
    text-decoration: underline;
  }
  
  #blog .entry .comments-tags-categories .like, #blog .entry .comments-tags-categories .reblog {
      float: left;
      margin-top: 5px;
      margin-right: 8px;
      line-height: 13px;
  }
  #blog .entry .comments-tags-categories .like a, #blog .entry .comments-tags-categories .reblog a {
      float: left;
  }
  #blog .entry .comments-tags-categories .like a .icon, #blog .entry .comments-tags-categories .reblog a .icon {
      padding: 3px;
      background-color: #d46819;
      float: left;
      margin-right: 3px;
      margin-left: 3px;
      -webkit-border-radius: 2px;
      -moz-border-radius: 2px;
      border-radius: 2px;
  }
  #blog .entry .comments-tags-categories .like a img, #blog .entry .comments-tags-categories .reblog a img {
      opacity: .8;
      float: left;
  }
  #blog .entry .comments-tags-categories .like a:hover img, #blog .entry .comments-tags-categories .reblog a:hover img {
      opacity: 1;
  }

